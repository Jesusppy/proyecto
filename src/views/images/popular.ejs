<div class="card mb-2">
    <div class="class-header bg-dark text-white">
         <h5><i class="fas fa-fire"> Mas popular</i></h5>
    </div>
    <div class="card-body">
        <div class="row">
            <% for (let p of sidebar.popular) {%> 
                <div class="col-md-4">
                    <a href="/images/<%= p.uniqueId %> ">
                        <img src="/public/upload/<%= p.filename %>" class="w-100 h-100 image-thumbail">
                    </a>
                </div>
            <% } %> 

        </div>
    </div>
</div>