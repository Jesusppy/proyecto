                    <a href="messages.html"><img src="/public/images/menu-icon/icon-2.png" alt="Icon"></a>
                    <a href="overview.html"><img src="/public/images/menu-icon/icon-3.png" alt="Icon"></a>
                    <a href="engagement.html"><img src="/public/images/menu-icon/icon-4.png" alt="Icon"></a>
                    <a href="traffic-conversion.html"><img src="/public/images/menu-icon/icon-5.png" alt="Icon"></a>
                </div>
            </div>
            <!-- Sidebar Wrapper End -->

            <div class="section section-padding mt-n10">
            <div class="container">

                <div class="row gx-10">
                    <div class="col-lg-8">

                        <!-- Blog Wrapper Start -->
                        <div class="blog-wrapper">
                            <div class="row">
                                <% for (let image of images) {%>
                                <div class="col-md-6">

                                    <!-- Single Blog Start -->
                                    <div class="single-blog">
                                        <div class="blog-image">
                                            <a href="/images/<%= image.uniqueId %> ">
                                                <img src="/public/upload/<%= image.filename %> " alt=""
                                                    class="w-100 h-100 img-thumbnail">
                                            </a>
                                        </div>
                                        <div class="blog-content">
                                            <div class="blog-author">
                                                <div class="author">

                                                    <h4 class="title"><a href="/images/<%= image.uniqueId %> "><%= image.title  %></a></h4>
                                                </div>
                                                <div class="tag">
                                                    <a href="/images/<%= image.uniqueId %> "> Comentar </a>
                                                </div>
                                            </div>

  
                                            <div class="blog-meta">
                                                <span> <i class="icofont-calendar"></i> <%= image.timestamp %> </span>
                                                <span> <i class="icofont-heart"></i><%= image.views %> </span>
                                            </div>

                                            <a href="/images/<%= image.uniqueId %> " class="btn btn-secondary btn-hover-primary">Leer mas</a>
                                        </div>
                                    </div>
                                    <!-- Single Blog End -->

                                </div>
                                <% } %>



                            </div>
                        </div>
                        <!-- Blog Wrapper End -->

                        <!-- Page Pagination End -->
                        <div class="page-pagination">
                            <ul class="pagination justify-content-center">
                                <li><a href="#"><i class="icofont-rounded-left"></i></a></li>
                                <li><a class="active" href="#">1</a></li>
                                <li><a href="#">2</a></li>
                                <li><a href="#">3</a></li>
                                <li><a href="#"><i class="icofont-rounded-right"></i></a></li>
                            </ul>
                        </div>
                        <!-- Page Pagination End -->
                        <form action="/images/imagenes" method="POST" enctype="multipart/form-data">
                            <div class="message-form">
                                <div class="auhtor">
                                    <img src="/public/images/author/author-16.jpg" alt="Author">
                                </div>
                                
                                <div class="message-input">
                                    <div class="form-data">
                                        <input type="text" name="title" class="form-control" placeholder="Titulo">
                                    </div>

                                    <div class="form-group">
                                    <textarea name="description" class="form-control" rows="2" id="MyID" placeholder="Añade un Descripción"></textarea>
                                    </div>

                                    <div class="message-btn">
                                        <div class="form-group">
                                            <div class="input-group">
                                                <div class="custom-file">
                                                    <input type="file" name="image" class="custom-file-input" id="inputGroupFile04"
                                                        aria-describedby="inputGroupFileAddon04">
                                                    <label class="custom-file-label" for="inputGroupFile04">Choose file</label>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <button class="btn btn-secondary btn-hover-primary">Cancel</button>
                                        <button class="btn btn-primary btn-hover-dark">Submit</button>
                                    </div>
                                </div>
                            </div>
                            
                        </form>
                    </div>
                    <div class="col-lg-4">

                        <!-- Blog Sidebar Start -->
                        <div class="sidebar">

                            <!-- Sidebar Widget Search Start -->
                            <div class="sidebar-widget widget-search">
