<% if (error) {%> 
  <ul> <% for (var i=0; i< error.length; i++) {%> </ul>

    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        <strong><div><%= error %> </div></strong> You should check in on some of those fields below.
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
  <% } %> 
  
<% } %> 



<%  if(locals.errors) { %>

    <ul> <% for (var i=0; i< errors.length; i++) {%> </ul>

    <div class="alert alert-warning alert-dismissible fade show" role="alert">
        <strong><%= errors[i].text %></strong> You should check in on some of those fields below.
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
      </div>
  <% } %> 
<% } %>