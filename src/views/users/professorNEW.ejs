                                    </div>

                                    <div class="content-wrapper">

                                        <div class="content-box">
                                            <a href="/homeworks/turn/<%= homeworks[i]._id %>"><span class="<%= homeworks[i].active ? 'btn btn-success' : 'btn btn-dark'%>">Activa</span></a>
                                        </div>

                                        <div class="content-box">
                                            <span class="count" > <a href="/homeworks/edit/<%= homeworks[i]._id %> ">Editar</a></span>
                                        </div>

                                        <div class="content-box">
                                            
                                            <form action="/homeworks/delete/<%= homeworks[i]._id %>?_method=DELETE"
                                                method="POST">
                                                <input type="hidden" name="_method" value="DELETE">
                                                <button class="btn btn-danger btn-block btn-sm" type="submit">Eliminar
                                                </button>
                                            </form>
                                            </span>
                                        </div>

                                        <div class="courses-select">
